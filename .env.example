# Environment variables for SPLADE Model Trainer
# Copy this file to .env and fill in your own values

# Disable tokenizers parallelism to avoid warnings and potential issues
TOKENIZERS_PARALLELISM=false

# Hugging Face API token for accessing private models and pushing models
HF_TOKEN=your_huggingface_token_here

# Groq API configuration for embedding generation (if needed)
GROQ_API_KEY=your_groq_api_key_here
GROQ_API_URL=https://api.groq.com/openai/v1/

# OpenAI API configuration for embedding generation (if needed)
OPENAI_API_KEY=your_openai_api_key_here
